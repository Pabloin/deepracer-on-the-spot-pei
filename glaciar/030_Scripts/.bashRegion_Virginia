

export MODEL_PATH=~/deepracer-on-the-spot-pei/glaciar/012_PEI_Models

function model() {

    echo "cd ${MODEL_PATH}/T2023-10/AWS-Tracks/.config_{$MODEL_VERSION_NUMBER}" 

    read -p "Â¿Modelo Version Numbre?  (Ejemplo: "C01a" o "C21a"? " MODEL_VERSION_NUMBER


    cd ${MODEL_PATH}/T2023-10/AWS-Tracks/.config_$MODEL_VERSION_NUMBER

}




